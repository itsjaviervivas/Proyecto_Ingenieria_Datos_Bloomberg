---CÃ³digos necesarios para insertar correctamente los datos en cada tabla

-- Ticker
 
COPY ticker (ticker, company) FROM 'C:\Proyecto_Ing\Ticker_F.csv'
WITH DELIMITER ',' CSV HEADER;

--Sector

COPY sector (sector, id_sector) FROM 'C:\Proyecto_Ing\Sector.csv'
WITH DELIMITER ',' CSV HEADER;

--Country

COPY WLS_country (name, weight, shares, id_country, PIB) 
FROM 'C:\Proyecto_Ing\WLS_COUNTRIES.csv' WITH DELIMITER ',' CSV HEADER;

--WLS_company

COPY WLS_company (id_ticker_company, fecha_renovacion, weight, shares, price, sector, country, revenue, total_return) 
FROM 'C:\Proyecto_Ing\WLS.csv' WITH DELIMITER ',' CSV HEADER;

--S_and_P

COPY S_and_P ( id_ticker_sp,fecha, price,id_country,sector_company, revenue) 
FROM 'C:\Proyecto_Ing\SAP.csv' WITH DELIMITER ',' CSV HEADER;
